@model IEnumerable<SimpleMVC.Models.Book>

@{
    ViewBag.Title = "Books";
    Layout = "~/Views/MasterLayout.cshtml";
}



    <main role="main" class="container">

        <div class="starter-template">
            <h1>BOOKS</h1>
            <p class="lead">

                <div>
                    @foreach (var item in Model)
                    {

                        @item.id
                        <span>-</span>
                        @item.title
                        <br>
                    }
                </div>
            </p>
        </div>

    </main>
    
